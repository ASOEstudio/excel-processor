{
  "name": "excel-processor",
  "version": "0.14.1",
  "main": "main.js",
  "description": "Spreadsheet Processor",
  "author": "ASO Est√∫dio",
  "license": "MIT",
  "build": {
    "productName": "Excel Processor"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "electron": "electron .",
    "start:electron": "ng build --base-href ./ && electron .",
    "build:electron": "ng build --base-href ./ && electron . && electron-packager . --platform win32 --arch x64 --overwrite --ignore=/.gitignore --ignore=/node_modules --ignore=/installers --ignore=/e2e --ignore=/src --ignore=/.browserslistrc --ignore=/.editorconfig --ignore=/angular.json --ignore=/angular.json --ignore=/karma.conf.js --ignore=/package-lock.json --ignore=/README.md --ignore=/tsconfig.app.json --ignore=/tsconfig.json --ignore=/tsconfig.spec.json --ignore=/tslint.json",
    "setup:electron": "electron-installer-windows --src excel-processor-win32-x64 --dest installers/ --options.noMsi=true",
    "build:setup": "npm run build:electron && npm run setup:electron",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "ng-table-virtual-scroll": "^1.3.8",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "xlsx": "*",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^10.1.1",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "xlsx": "*"
  }
}
